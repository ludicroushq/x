{
  "name": "@xframework/queue",
  "version": "0.1.2",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./pg-boss": {
      "types": "./dist/pg-boss.d.ts",
      "require": "./dist/pg-boss.cjs",
      "import": "./dist/pg-boss.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "pg-boss": ">=10.0.0"
  },
  "peerDependenciesMeta": {
    "pg-boss": {
      "optional": true
    }
  },
  "dependencies": {
    "@xframework/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
