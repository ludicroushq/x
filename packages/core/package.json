{
  "name": "@xframework/core",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "main": "./dist/index.js",
      "module": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/types.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/types.ts --format cjs,esm --dts --watch",
    "lint": "eslint . --max-warnings 0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@xframework/eslint-config": "workspace:*",
    "@xframework/typescript-config": "workspace:*",
    "eslint": "^8.57.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "hono": "^4.5.8"
  }
}
